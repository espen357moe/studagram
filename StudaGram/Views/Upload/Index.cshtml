@model StudaGram.Models.UploadModel
@{
    ViewBag.Title = "Index";
}

    <h2>Upload image</h2>
    <form method="post" action="" enctype="multipart/form-data">
        <div id="imgInfo">
            @Html.TextBoxFor(m => m.Title)
            @Html.TextBoxFor(m => m.Description)          
        </div>

        <input type="file" id="@Html.IdFor(m => m.UploadedFile)" name="@Html.NameFor(m => m.UploadedFile)"/>)
        <input class="btn btn-primary btn-lg" type="submit"/>
    </form>


